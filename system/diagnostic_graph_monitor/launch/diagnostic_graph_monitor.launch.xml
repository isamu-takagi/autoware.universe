<launch>
  <arg name="graph_file" default="$(find-pkg-share diagnostic_graph_monitor)/config/autoware.yaml"/>
  <include file="$(find-pkg-share diagnostic_graph_aggregator)/launch/diagnostic_graph_aggregator.launch.xml">
    <arg name="graph_file" value="$(var graph_file)"/>
  </include>
  <node pkg="diagnostic_graph_monitor" exec="component_state_diagnostics" name="component_state_diagnostics"/>
</launch>
